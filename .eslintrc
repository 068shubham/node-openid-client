{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_claim_names", "_claim_sources"] }],
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "no-param-reassign": ["error", { "props": false }],
    "prefer-rest-params": "off",
    "prefer-spread": "off",
    "strict": "off"
  }
}
